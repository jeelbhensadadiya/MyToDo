<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nv_graph"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.jeelpatel.mytodo.ui.view.fragments.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main">
        <action
            android:id="@+id/action_mainFragment_to_recycleBinFragment"
            app:destination="@id/recycleBinFragment" />
        <action
            android:id="@+id/action_mainFragment_to_createTaskFragment"
            app:destination="@id/createTaskFragment" />
        <action
            android:id="@+id/action_mainFragment_to_remoteTodoFragment"
            app:destination="@id/remoteTodoFragment" />
        <action
            android:id="@+id/action_mainFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_mainFragment_to_taskViewFragment"
            app:destination="@id/taskViewFragment" />
    </fragment>

    <fragment
        android:id="@+id/recycleBinFragment"
        android:name="com.jeelpatel.mytodo.ui.view.fragments.RecycleBinFragment"
        android:label="fragment_recycle_bin"
        tools:layout="@layout/fragment_recycle_bin">
        <action
            android:id="@+id/action_recycleBinFragment_to_taskViewFragment"
            app:destination="@id/taskViewFragment" />
    </fragment>

    <fragment
        android:id="@+id/createTaskFragment"
        android:name="com.jeelpatel.mytodo.ui.view.fragments.CreateTaskFragment"
        android:label="fragment_create_task"
        tools:layout="@layout/fragment_create_task" />

    <fragment
        android:id="@+id/remoteTodoFragment"
        android:name="com.jeelpatel.mytodo.ui.view.fragments.RemoteTodoFragment"
        android:label="fragment_remote_todo"
        tools:layout="@layout/fragment_remote_todo" />

    <fragment
        android:id="@+id/taskViewFragment"
        android:name="com.jeelpatel.mytodo.ui.view.fragments.TaskViewFragment"
        android:label="fragment_task_view"
        tools:layout="@layout/fragment_task_view">
        <argument
            android:name="taskId"
            app:argType="integer" />

        <argument
            android:name="title"
            app:argType="string" />

        <argument
            android:name="desc"
            app:argType="string" />

        <argument
            android:name="dueDate"
            app:argType="long" />

        <argument
            android:name="priority"
            app:argType="integer" />

        <argument
            android:name="isCompleted"
            app:argType="boolean" />
    </fragment>

    <fragment
        android:id="@+id/profileFragment"
        android:name="com.jeelpatel.mytodo.ui.view.fragments.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile">
        <action
            android:id="@+id/action_profileFragment_to_cameraFragment"
            app:destination="@id/cameraFragment" />
        <action
            android:id="@+id/action_profileFragment_to_mediaListFragment"
            app:destination="@id/mediaListFragment" />
        <action
            android:id="@+id/action_profileFragment_to_servicesFragment"
            app:destination="@id/servicesFragment" />
        <action
            android:id="@+id/action_profileFragment_to_simpleNotificationFragment"
            app:destination="@id/simpleNotificationFragment" />
        <action
            android:id="@+id/action_profileFragment_to_workManagerFragment"
            app:destination="@id/workManagerFragment" />
        <action
            android:id="@+id/action_profileFragment_to_sensorsFragment"
            app:destination="@id/sensorsFragment" />
        <action
            android:id="@+id/action_profileFragment_to_connectivityFragment"
            app:destination="@id/connectivityFragment" />
        <action
            android:id="@+id/action_profileFragment_to_galleryFragment"
            app:destination="@id/galleryFragment" />
        <action
            android:id="@+id/action_profileFragment_to_photoPickerFragment"
            app:destination="@id/photoPickerFragment" />
        <action
            android:id="@+id/action_profileFragment_to_testFragment"
            app:destination="@id/testFragment" />
    </fragment>

    <fragment
        android:id="@+id/cameraFragment"
        android:name="com.jeelpatel.mytodo.ui.view.fragments.otherFeatures.CameraFragment"
        android:label="fragment_camera"
        tools:layout="@layout/fragment_camera" />

    <fragment
        android:id="@+id/mediaListFragment"
        android:name="com.jeelpatel.mytodo.ui.view.fragments.otherFeatures.MediaListFragment"
        android:label="fragment_media_list"
        tools:layout="@layout/fragment_media_list">
        <action
            android:id="@+id/action_mediaListFragment_to_audioPlayerFragment"
            app:destination="@id/audioPlayerFragment" />
        <action
            android:id="@+id/action_mediaListFragment_to_videoPlayerFragment"
            app:destination="@id/videoPlayerFragment" />
    </fragment>

    <fragment
        android:id="@+id/servicesFragment"
        android:name="com.jeelpatel.mytodo.ui.view.fragments.otherFeatures.ServicesFragment"
        android:label="fragment_services"
        tools:layout="@layout/fragment_services" />

    <fragment
        android:id="@+id/audioPlayerFragment"
        android:name="com.jeelpatel.mytodo.ui.view.fragments.otherFeatures.AudioPlayerFragment"
        android:label="fragment_audio_player"
        tools:layout="@layout/fragment_audio_player">
        <argument
            android:name="audioUrl"
            app:argType="string" />

    </fragment>

    <fragment
        android:id="@+id/videoPlayerFragment"
        android:name="com.jeelpatel.mytodo.ui.view.fragments.otherFeatures.VideoPlayerFragment"
        android:label="fragment_video_player"
        tools:layout="@layout/fragment_video_player">

        <argument
            android:name="videoUrl"
            app:argType="string" />
    </fragment>

    <fragment
        android:id="@+id/simpleNotificationFragment"
        android:name="com.jeelpatel.mytodo.ui.view.fragments.otherFeatures.SimpleNotificationFragment"
        android:label="fragment_simple_notification"
        tools:layout="@layout/fragment_simple_notification" />

    <fragment
        android:id="@+id/workManagerFragment"
        android:name="com.jeelpatel.mytodo.ui.view.fragments.otherFeatures.WorkManagerFragment"
        android:label="WorkManagerFragment" />

    <fragment
        android:id="@+id/sensorsFragment"
        android:name="com.jeelpatel.mytodo.ui.view.fragments.otherFeatures.SensorsFragment"
        android:label="fragment_sensors"
        tools:layout="@layout/fragment_sensors">
        <action
            android:id="@+id/action_sensorsFragment_to_gyroscopeFragment"
            app:destination="@id/gyroscopeFragment" />
    </fragment>

    <fragment
        android:id="@+id/gyroscopeFragment"
        android:name="com.jeelpatel.mytodo.ui.view.fragments.otherFeatures.GyroscopeFragment"
        android:label="fragment_gyroscope"
        tools:layout="@layout/fragment_gyroscope" />

    <fragment
        android:id="@+id/connectivityFragment"
        android:name="com.jeelpatel.mytodo.ui.view.fragments.otherFeatures.ConnectivityFragment"
        android:label="fragment_connectivity"
        tools:layout="@layout/fragment_connectivity">
        <action
            android:id="@+id/action_connectivityFragment_to_wifiFragment"
            app:destination="@id/wifiFragment" />
        <action
            android:id="@+id/action_connectivityFragment_to_bluetoothFragment"
            app:destination="@id/bluetoothFragment" />
    </fragment>

    <fragment
        android:id="@+id/wifiFragment"
        android:name="com.jeelpatel.mytodo.ui.view.fragments.otherFeatures.WifiFragment"
        android:label="fragment_wifi"
        tools:layout="@layout/fragment_wifi" />

    <fragment
        android:id="@+id/bluetoothFragment"
        android:name="com.jeelpatel.mytodo.ui.view.fragments.otherFeatures.BluetoothFragment"
        android:label="fragment_bluetooth"
        tools:layout="@layout/fragment_bluetooth" />

    <fragment
        android:id="@+id/galleryFragment"
        android:name="com.jeelpatel.mytodo.ui.view.fragments.otherFeatures.GalleryFragment"
        android:label="fragment_gallery"
        tools:layout="@layout/fragment_gallery" />
    <fragment
        android:id="@+id/photoPickerFragment"
        android:name="com.jeelpatel.mytodo.ui.view.fragments.otherFeatures.PhotoPickerFragment"
        android:label="fragment_photo_picker"
        tools:layout="@layout/fragment_photo_picker" />
    <fragment
        android:id="@+id/testFragment"
        android:name="com.jeelpatel.mytodo.ui.view.fragments.otherFeatures.TestFragment"
        android:label="fragment_test"
        tools:layout="@layout/fragment_test" />

</navigation>