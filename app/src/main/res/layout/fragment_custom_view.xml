<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.view.fragments.otherFeatures.CustomViewFragment">

    <com.jeelpatel.mytodo.ui.customView.CircularProgressView
        android:id="@+id/circleProgress"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_gravity="center"
        app:cp_backgroundColor="#E0E0E0"
        app:cp_progress="0"
        app:cp_progressColor="@color/material_dynamic_neutral30"
        app:cp_strokeWidth="18dp"
        tools:ignore="MissingClass" />

</FrameLayout>